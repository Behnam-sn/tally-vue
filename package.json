    "@vue/tsconfig": "^0.1.3",
    "electron": "^18.2.3",
    "eslint": "^8.15.0",
    "eslint-plugin-vue": "^8.7.1",
    "prettier": "^2.6.2",
    "sass": "^1.51.0",
